# Used by xxx.adl to blink "Moving" indicator.
record(calc, "$(P)alldoneBlink") {
  field(SCAN, ".5 second")
  field(CALC, "A?0:!B")
  field(INPA, "$(P)alldone")
  field(INPB, "$(P)alldoneBlink")
}

# Allow smart clients to maintain a list of moving motors
